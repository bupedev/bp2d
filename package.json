{
  "name": "bp2d",
  "version": "0.3.0",
  "description": "A UMD geometry library written in Typescript ",
  "main": "index.js",
  "scripts": {
    "all": "npm run build && npm run tests && npm run docs",
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
    "tests": "jest --coverage --verbose",
    "docs": "npx typedoc --excludePrivate --githubPages false --out docs src/index.ts"
  },
  "author": "Ben Lewis <ben@bupe.dev>",
  "license": "MIT",
  "repository": "bupedev/bp2d",
  "exports": {
    ".": "./lib/index.js"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/p5": "^1.4.2",
    "p5": "^1.4.1",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.9",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "audit": "^0.0.6"
  }
}
